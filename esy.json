{
  "name": "ocaml",
  "version": "5.2.0-musl",
  "description": "OCaml compiler packaged for esy",
  "source": "https://github.com/ocaml/ocaml/archive/5.2.0.tar.gz#sha256:48554abfd530fcdaa08f23f801b699e4f74c320ddf7d0bd56b0e8c24e55fc911",
  "override": {
    "build": [
      "./configure LIBS=-static CFLAGS=-Os --prefix #{self.install}",
      "make -j#{self.jobs}"
    ],
    "install": "make install",
    "buildsInSource": true,
    "exportedEnv": {
      "OCAMLPATH": {
        "val": "#{ocaml.lib / 'ocaml' : $OCAMLPATH}",
        "scope": "global"
      },
      "OCAMLLIB": {
        "val": "#{ocaml.lib / 'ocaml'}",
        "scope": "global"
      },
      "CAML_LD_LIBRARY_PATH": {
        "val": "#{ocaml.lib / 'ocaml' / 'stublibs' : ocaml.lib / 'ocaml' : $CAML_LD_LIBRARY_PATH}",
        "scope": "global"
      },
      "OCAML_TOPLEVEL_PATH": {
        "val": "#{ocaml.lib / 'ocaml'}",
        "scope": "global"
      }
    },
    "dependencies": {}
  }
}
